    "semver": "7.3.5",
    "typescript": "4.4.3"
  },
  "devDependencies": {
    "@types/jasmine": "3.9.1",
    "@types/node": "16.10.2",
    "@types/semver": "7.3.8",
    "chalk": "4.1.2",
    "istanbul": "0.4.5",
    "jasmine": "3.9.0"
  },
  "ng-add": {
    "save": "devDependencies"
  },
  "schematics": "./src/collection.json"
