{
  "name": "@oktadev/schematics",
  "version": "2.2.0",
  "description": "Schematics for Okta Auth",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "coverage": "istanbul cover -x 'src/**/*_spec.js' jasmine src/**/*_spec.js",
    "prepublishOnly": "npm run build",
    "check-deps": "ncu --packageFile package-sdks.json"
  },
  "keywords": [
    "schematics",
    "authentication",
    "authorization",
    "oidc",
    "oauth",
    "okta",
    "angular",
    "react",
    "vue",
    "ionic",
    "react-native",
    "express",
    "typescript"
  ],
  "author": "OktaDev <devrel@okta.com>",
  "license": "Apache-2.0",
  "repository": "git@github.com:oktadeveloper/schematics.git",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "10.1.0",
    "@angular-devkit/schematics": "10.0.8",
    "@schematics/angular": "10.1.0",
    "rxjs": "6.6.2",
    "schematics-utilities": "2.0.2"
  },
  "devDependencies": {
    "@types/jasmine": "3.5.14",
    "@types/node": "14.6.4",
    "chalk": "4.1.0",
    "istanbul": "0.4.5",
    "jasmine": "3.6.1",
    "typescript": "4.0.2"
  },
  "resolutions": {
    "handlebars": "4.5.3"
  }
