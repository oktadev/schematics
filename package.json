    "rxjs": "7.8.1",
    "semver": "7.5.4",
    "typescript": "5.3.2"
  },
  "devDependencies": {
    "@types/jasmine": "5.1.0",
    "@types/node": "20.11.3",
    "@types/semver": "7.5.1",
    "chalk": "5.3.0",
    "istanbul": "0.4.5",
    "jasmine": "5.1.0"
  },
  "ng-add": {
    "save": "devDependencies"
  },
  "schematics": "./src/collection.json"
